<docking:Workspace
    x:Class="WindowsClient.ApplicationLayout.TabbedWindowWorkspace.TabbedWindowWorkspaceView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:docking="http://schemas.actiprosoftware.com/winfx/xaml/docking"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <docking:TabbedMdiHost>
        <docking:TabbedMdiContainer x:Name="tabControl" HasNewTabButton="True">
            <docking:DocumentWindow Title="Getting Started">
                <StackPanel VerticalAlignment="Center" Orientation="Vertical">
                    <Image
                        Height="64"
                        HorizontalAlignment="Center"
                        Source="/ApplicationLayout/Icons/BoxSearch.png" />
                    <TextBlock
                        HorizontalAlignment="Center"
                        FontSize="50"
                        FontWeight="DemiBold"
                        Foreground="#ccc"
                        Text="Material Balance Model" />
                </StackPanel>
            </docking:DocumentWindow>
        </docking:TabbedMdiContainer>
    </docking:TabbedMdiHost>
</docking:Workspace>
